{
  "name": "@iamrony777/baileys",
  "version": "6.7.19-2",
  "type": "module",
  "description": "A WebSockets library for interacting with WhatsApp Web",
  "keywords": [
    "whatsapp",
    "baileys",
    "whatsapp-api",
    "mongodb",
    "redis",
    "automation",
    "store",
    "auth",
    "new",
    "nodejs",
    "typescript",
    "multi-device",
    "WhatsApp-web",
    "javascript"
  ],
  "homepage": "https://github.com/iamrony777/Baileys",
  "repository": {
    "url": "git@github.com:iamrony777/Baileys.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "license": "MIT",
  "author": "Rajeh Taher",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*",
    "WAProto/**/*",
    "engine-requirements.js"
  ],
  "scripts": {
    "build:all": "npm run build && npm run build:docs",
    "build:docs": "typedoc",
    "build": "tsc -P tsconfig.build.json && tsc-esm-fix --tsconfig=tsconfig.build.json --ext=.js",
    "changelog:last": "conventional-changelog -p angular -r 2",
    "changelog:preview": "conventional-changelog -p angular -u",
    "changelog:update": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "example": "tsx --inspect Example/example.ts",
    "example:node": "node --inspect -r ts-node/register Example/example.ts",
    "example:mobile": "node --inspect -r ts-node/register Example/example.ts --mobile",
    "gen:protobuf": "sh WAProto/GenerateStatics.sh",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "lint": "tsc && eslint src --ext .js,.ts",
    "lint:fix": "npm run format && npm run lint --fix",
    "prepack": "npm run build",
    "prepare": "npm run build",
    "preinstall": "node ./engine-requirements.js",
    "release": "release-it",
    "test": "jest",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org rony-wp-bot --project baileys ./lib && sentry-cli sourcemaps upload --org rony-wp-bot --project baileys ./lib"
  },
  "dependencies": {
    "@adiwajshing/keyed-db": "^0.2.4",
    "@cacheable/node-cache": "^1.4.0",
    "@hapi/boom": "^10.0.1",
    "@whiskeysockets/eslint-config": "github:whiskeysockets/eslint-config",
    "async-lock": "^1.4.1",
    "async-mutex": "^0.5.0",
    "audio-decode": "^2.1.5",
    "axios": "^1.6.1",
    "cache-manager": "^5.7.6",
    "cron": "^3.1.6",
    "dotenv": "^16.3.1",
    "futoin-hkdf": "^1.5.3",
    "jimp": "^0.22.10",
    "libphonenumber-js": "^1.10.49",
    "libsignal": "github:WhiskeySockets/libsignal-node",
    "link-preview-js": "^3.0.5",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.43",
    "mongodb": "^6.17.0",
    "music-metadata": "^7.14.0",
    "node-cache": "^5.1.2",
    "pino": "^8.16.1",
    "protobufjs": "^7.2.5",
    "qrcode-terminal": "^0.12.0",
    "react": "^18.2.0",
    "redis": "^5.5.6",
    "sharp": "^0.32.6",
    "uuid": "^10.0.0",
    "ws": "^8.14.2"
  },
  "peerDependencies": {
    "@hapi/boom": "^9.1.3",
    "@types/jest": "^27.5.1",
    "@types/node": "^16.0.0",
    "@types/ws": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "async-mutex": "^0.5.0",
    "audio-decode": "^2.1.5",
    "axios": "^1.6.0",
    "jimp": "^1.6.0",
    "libsignal": "*",
    "link-preview-js": "^3.0.0",
    "music-metadata": "^11.7.0",
    "pino": "^9.6",
    "protobufjs": "^7.2.4",
    "sharp": "*",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.31.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^16.0.0",
    "@types/ws": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "@whiskeysockets/eslint-config": "github:whiskeysockets/eslint-config",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.4.0",
    "jest": "^29.7.0",
    "jimp": "^1.6.0",
    "jiti": "^2.4.2",
    "json": "^11.0.0",
    "link-preview-js": "^3.0.5",
    "open": "^8.4.2",
    "prettier": "^3.5.3",
    "protobufjs-cli": "^1.1.3",
    "qrcode-terminal": "^0.12.0",
    "release-it": "^15.10.3",
    "ts-jest": "^29.3.2",
    "tsc-esm-fix": "^3.1.2",
    "tsx": "^4.20.3",
    "typedoc": "^0.27.9",
    "typedoc-plugin-markdown": "4.4.2",
    "typescript": "^5.8.2"
  },
  "peerDependenciesMeta": {
    "audio-decode": {
      "optional": true
    },
    "jimp": {
      "optional": true
    },
    "link-preview-js": {
      "optional": true
    }
  },
  "packageManager": "yarn@4.9.2",
  "engines": {
    "node": ">=20.0.0"
  }
}
